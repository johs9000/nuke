 addUserKnob {41 first_frame l "frame range" T jr_srcRange12_master.first_frame}
 addUserKnob {41 last_frame l "" -STARTLINE T jr_srcRange12_master.last_frame}
 addUserKnob {41 reset T jr_srcRange12_master.reset}
